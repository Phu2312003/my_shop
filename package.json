{
  "name": "my-shop",
  "version": "0.1.0",
  "private": true,
  "homepage": "/my_shop/",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:analyze": "npm run build -- --stats && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "craco test",
    "eject": "react-scripts eject",
    "vercel-build": "craco build",
    "serve": "serve -s build -l 3000",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0"
  }
}
